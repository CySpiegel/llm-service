# HuggingFace Token (needed for vLLM with official Meta models)
HF_TOKEN=

# PostgreSQL for all services
POSTGRES_USER=openwebui
POSTGRES_PASSWORD=
POSTGRES_DB=openwebui
LITELLM_DB=litellm
N8N_DB=n8n
LANGFUSE_DB=langfuse


# For Litellm UI
LITELLM_MASTER_KEY=your_litellm_master_key
LITELLM_UI_USERNAME=
LITELLM_UI_PASSWORD=

# For Grafana
GRAFANA_PASSWORD=

# For Neo4j
NEO4J_USER=neo4j
NEO4J_PASSWORD=!

# For N8N
N8N_BASIC_AUTH_USER=spiegel
N8N_BASIC_AUTH_PASSWORD=

# For Langfuse
LANGFUSE_SECRET=your_langfuse_nextauth_secret

# Optional: Custom secrets for other services as needed
FLOWISE_USERNAME=
FLOWISE_PASSWORD=

# If adding Clickhouse/Minio/Supabase
CLICKHOUSE_PASSWORD=
MINIO_ROOT_PASSWORD=
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# --- Langfuse ---
LANGFUSE_SALT=your_random_salt_here
# Key run openssl rand -hex 32
ENCRYPTION_KEY=
NEXTAUTH_SECRET=your_nextauth_secret_here

# (Optional Langfuse project/org/user inits)
LANGFUSE_INIT_ORG_ID=
LANGFUSE_INIT_ORG_NAME=
LANGFUSE_INIT_PROJECT_ID=
LANGFUSE_INIT_PROJECT_NAME=
LANGFUSE_INIT_PROJECT_PUBLIC_KEY=
LANGFUSE_INIT_PROJECT_SECRET_KEY=
LANGFUSE_INIT_USER_EMAIL=
LANGFUSE_INIT_USER_NAME=
LANGFUSE_INIT_USER_PASSWORD=

# --- ClickHouse ---
CLICKHOUSE_PASSWORD=your_clickhouse_password

# --- Minio ---
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=your_minio_root_password

# --- Langfuse Features (optional, you can leave as-is or set as needed) ---
TELEMETRY_ENABLED=false
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=false
CLICKHOUSE_CLUSTER_ENABLED=false
LANGFUSE_S3_BATCH_EXPORT_ENABLED=false
